{
    "name": "Pluto.tv ChromeCast Volume",
    "description": "Ability to change ChromeCast volume using Pluto.TV",
    "version": "0.1",
    "permissions": [
        "tabs",
        "http://pluto.tv/*",
        "https://pluto.tv/*"
    ],

    "content_scripts": [
        {
        "matches": [
            "http://pluto.tv/*",
            "https://pluto.tv/*"
        ],
        "js": ["injector.js"],
        "run_at": "document_end"
        }
    ],
    "web_accessible_resources": [
        "content.js"
    ],
    "manifest_version":2
}
